FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 373.33;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           438.1;
    }

    outlet
    {
        type            zeroGradient;
    }

    symBoundTop
    {
        type            symmetryPlane;
    }

    symBoundBottom
    {
        type            symmetryPlane;
    }

    flatPlate
    {
        type            fixedValue;
        value           1e7;
    }

    frontAndBack
    {
        type            empty;
    }
}


